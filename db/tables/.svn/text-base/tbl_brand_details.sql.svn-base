DROP TABLE IF EXISTS tbl_brand_details;
CREATE TABLE tbl_brand_details
(
  brand_id INT(10) UNSIGNED NOT NULL auto_increment,
  brand_name VARCHAR(255) NOT NULL,
  brand_description VARCHAR(1024) DEFAULT NULL,
  brand_email VARCHAR(255) DEFAULT NULL,
  brand_add VARCHAR(1024) DEFAULT NULL,
  brand_cont_no VARCHAR(32) DEFAULT NULL,
  disc_type VARCHAR(32) DEFAULT NULL,
  good_type VARCHAR(255) DEFAULT NULL,
  disc_available INT(11) UNSIGNED DEFAULT NULL,
  current_order tinyint(3) unsigned,
  PRIMARY KEY (brand_id),
  UNIQUE KEY brand_id (brand_id)
);

